{
  "name": "@databrokerglobal/dxc",
  "version": "1.0.0",
  "description": "Contract for the DXC platform",
  "main": "truffle-config.js",
  "scripts": {
    "compile": "npx truffle compile && npx typechain --target truffle-v5 './build/contracts/*.json' && npx tsc --declaration",
    "lint": "npx buidler check",
    "test": "truffle test",
    "coverage": "truffle run coverage",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "migrate": "npx waffle && npx truffle compile --all && npx typechain --target truffle-v5 './build/contracts/*.json' && npx tsc --declaration && npx truffle migrate"
  },
  "author": "Databroker",
  "license": "SEE LICENSE IN mint-license.txt",
  "bugs": {
    "url": "https://github.com/databrokerglobal/dxc/issues"
  },
  "homepage": "https://databroker.online/",
  "repository": {
    "type": "git",
    "url": "https://github.com/databrokerglobal/dxc.git"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.1.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@types/ethereumjs-abi": "^0.6.3",
    "axios": "^0.21.1",
    "bn.js": "^5.1.1",
    "dayjs": "^1.8.26",
    "dotenv": "^8.2.0",
    "ethereumjs-abi": "^0.6.8",
    "ethers": "^4.0.47",
    "faker": "^4.1.0",
    "truffle-hdwallet-provider": "^1.0.17"
  },
  "devDependencies": {
    "@nomiclabs/buidler": "^1.4.8",
    "@nomiclabs/buidler-solhint": "1.2.0",
    "@nomiclabs/buidler-truffle5": "1.2.0",
    "@nomiclabs/buidler-web3": "1.2.0",
    "@openzeppelin/contracts-upgradeable": "^4.2.0",
    "@openzeppelin/truffle-upgrades": "^1.5.0",
    "@typechain/truffle-v5": "0.9.0",
    "@types/faker": "4.1.11",
    "@types/mocha": "7.0.2",
    "@types/node": "13.13.4",
    "chai": "^4.3.4",
    "eth-gas-reporter": "^0.2.22",
    "ethereum-waffle": "^3.3.0",
