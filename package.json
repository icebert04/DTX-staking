    "truffle-typings": "1.0.8",
    "ts-generator": "0.0.8",
    "ts-node": "8.9.1",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-consistent-codestyle": "1.16.0",
